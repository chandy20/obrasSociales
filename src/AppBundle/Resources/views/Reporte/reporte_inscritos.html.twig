{%if datos %}
    <style>
        .titulo{
            height: 40px; 
            background-color: #5AC7FF;
        }
        .elemento1{
            background-color: gray;
            height: 20px; 
        }
        .elemento2{
            height: 20px; 
            background-color: white;
        }
    </style>
    <div class="col-md-12">
        <br>
        <br>
        <table class="text-center">
            <tr>
            <th class="titulo">Cuenta</th>
            <th class="titulo">Titular</th>
            <th class="titulo">Beneficiario</th>
            <th class="titulo">Seccional</th>
            </tr>
            {%set cuenta = 1%}
            {%for key, dato in datos%}
                {%for beneficiario in dato.beneficiarios%}
            <tr>
                    <td class="{% if cuenta is even %}elemento1{%else%}elemento2{%endif%}">{{cuenta}}</td>
                    <td class="{% if cuenta is even %}elemento1{%else%}elemento2{%endif%}">{{key}}</td>
                    <td class="{% if cuenta is even %}elemento1{%else%}elemento2{%endif%}">{{beneficiario.nombre[0]}}</td>
                    <td class="{% if cuenta is even %}elemento1{%else%}elemento2{%endif%}">{{beneficiario.seccional[0]}}</td>
            </tr>
                    {%set cuenta = cuenta +1 %}
                {%endfor%}
            {%endfor%}
        </table>
    </div>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
{%else%}
    <div>No se encontró información con los parametros de busqueda.</div>
{%endif%}
