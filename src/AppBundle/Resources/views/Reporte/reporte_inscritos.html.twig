{%if datos %}
    <style>
        .titulo{
            height: 40px; 
            background-color: #5AC7FF;
        }
        .elemento1{
            background-color: gray;
            height: 20px; 
        }
        .elemento2{
            height: 20px; 
            background-color: white;
        }
    </style>
    <div class="col-md-12">
        <br>
        <br>
        <div class="col-md-12 text-center">
            <div class="col-md-4 titulo">Titular</div>
            <div class="col-md-4 titulo">Beneficiario</div>
            <div class="col-md-4 titulo">Seccional</div>
            {%set cuenta = 1%}
            {%for key, dato in datos%}
                {%for beneficiario in dato.beneficiarios%}
                    <div class="col-md-4 {% if cuenta is even %}elemento1{%else%}elemento2{%endif%}">{{key}}</div>
                    <div class="col-md-4 {% if cuenta is even %}elemento1{%else%}elemento2{%endif%}">{{beneficiario.nombre[0]}}</div>
                    <div class="col-md-4 {% if cuenta is even %}elemento1{%else%}elemento2{%endif%}">{{beneficiario.seccional[0]}}</div>
                    {%set cuenta = cuenta +1 %}
                {%endfor%}
            {%endfor%}
        </div>
    </div>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
{%else%}
    <div>No se encontró información con los parametros de busqueda.</div>
{%endif%}
